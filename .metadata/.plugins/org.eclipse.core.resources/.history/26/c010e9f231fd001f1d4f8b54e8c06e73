package ar.edu.info.unlp.ejercicio2;

public class Piedra implements Opcion {

	@Override
	public String ganaContra(Opcion op) {
		return "La piedra se impone sobre el tijerismo";
	}

	@Override
	public String pierdeContra(Opcion op) {
		return "El papel se impone sobre el piedrismo";
	}

	@Override
	public String empataContra(Opcion op) {
		return "Tras una dura contienda, las piedras se retiran a sus hogares";
	}

}
